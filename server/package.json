{
  "name": "resume-agent-server",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "load:profile": "node scripts/load-profile.mjs",
    "load:profile:quick": "node scripts/load-profile.mjs --users=12 --read-requests=600 --read-concurrency=60 --sse-hold-users=12 --sse-hold-ms=5000 --sse-churn-requests=80 --sse-churn-concurrency=24 --pipeline-requests=24 --pipeline-concurrency=24",
    "load:profile:tiers": "node scripts/load-profile-tiers.mjs",
    "load:profile:tiers:quick": "node scripts/load-profile-tiers.mjs --tiers=small",
    "check:migrations": "node scripts/check-migration-drift.mjs --json=true",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck",
    "verify": "npm run typecheck && npm test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@hono/node-server": "^1.14.0",
    "@supabase/supabase-js": "^2.49.0",
    "hono": "^4.7.0",
    "pino": "^10.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
